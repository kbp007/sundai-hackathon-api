{
  "name": "sundai-hackathon-api",
  "version": "1.0.0",
  "description": "Public hackathon database with API key authentication and AI-powered team matching",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "setup": "node scripts/setup.js",
    "frontend": "cd frontend && npm start",
    "frontend:build": "cd frontend && npm run build",
    "dev:full": "concurrently \"npm run dev\" \"npm run frontend\""
  },
  "keywords": ["hackathon", "api", "team-matching", "ai", "supabase", "discord"],
  "author": "Sundai Hackathon Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "@supabase/supabase-js": "^2.38.4",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "discord.js": "^14.14.1",
    "openai": "^4.20.1",
    "joi": "^17.11.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "concurrently": "^8.2.2"
  }
} 